{
  "name": "next.js-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "preview": "serve build --listen 3000",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "cypress": "start-server-and-test preview http://localhost:3000 \"cypress open --e2e --browser chrome\"",
    "cypress:headless": "start-server-and-test preview http://localhost:3000 \"cypress run --e2e\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --check \"./**/*.{js,mjs,jsx,ts,tsx,html,json,md}\"",
    "format:fix": "prettier --write \"./**/*.{js,mjs,jsx,ts,tsx,html,json,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.19",
    "autoprefixer": "10.4.14",
    "classnames": "^2.3.2",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "tailwindcss": "3.3.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cypress": "^12.16.0",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-perfectionist": "^1.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "msw": "^1.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "serve": "^14.2.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
